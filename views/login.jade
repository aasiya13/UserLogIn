extends layout

block content
  h1 Register
  p Please login below
  form(method='post', action='/users/login', endtype='multipart/form-data')
    .form-group
      label User Name
      input.form-control(name='username', type = 'text')
    .form-group
      label Password
      input.form-control(name='password', type = 'password')
    input.btn.btn-default(name='submit' type = 'submit' value='Log In')
